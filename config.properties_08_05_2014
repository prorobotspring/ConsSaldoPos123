application.corte123.ivrsmstextinfo.timeOut=60000

# Metodo consultado
application.corte123.ivrsmstextinfo.soapaction.retrieve=getIVRSMSTextInfo

###################  Datos de configuracion para el sms sender (sms1:Consumido) ###################

